
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Permet la lecture et l'écriture sur n'importe quel document
    // pour les besoins du développement.
    // ATTENTION : Ne pas utiliser en production sans authentification !
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
